{
    "name": "LimitedPageCreator",
    "author": "DIQA Projektmanagement GmbH",
    "url": "https://www.diqa-pm.com",
    "description": "This extension implements a special page for createing new pages only in allowed namespaces.",
    "version": "0.1",
    "type": "specialpage ",
    "requires": {
        "MediaWiki": ">= 1.31.0",
        "extensions": {
            "Bootstrap": ">= 4.5.0"
        }
    },
    "SpecialPages": {
        "CreatePage": "DIQA\\Extension\\LimitedPageCreator\\SpecialLimitedPageCreator"
    },
    "AutoloadNamespaces": {
        "DIQA\\Extension\\LimitedPageCreator\\": "src/"
    },
    "MessagesDirs": {
        "LimitedPageCreator": [
            "i18n"
        ]
    },
    "manifest_version": 2
}
